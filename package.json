{
  "name": "pythia-shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-cor": "ng build @myrmidon/pythia-core",
    "build-api": "ng build @myrmidon/pythia-api",
    "build-ui": "ng build @myrmidon/pythia-ui",
    "build-dcr": "ng build @myrmidon/pythia-document-reader",
    "build-doc": "ng build @myrmidon/pythia-document-list",
    "build-trm": "ng build @myrmidon/pythia-term-list",
    "build-sta": "ng build @myrmidon/pythia-stats",
    "build-src": "ng build @myrmidon/pythia-search",
    "build-crp": "ng build @myrmidon/pythia-corpus-list",
    "build-qbl": "ng build @myrmidon/pythia-query-builder",
    "build-lib": "npm run build-cor && npm run build-api && npm run build-ui && npm run build-dcr && npm run build-doc && npm run build-trm && npm run build-sta && npm run build-src && npm run build-crp && npm run build-qbl",
    "xi18n": "ng extract-i18n --output-path src/locale && xliffmerge --profile xliffmerge.json it-IT"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.5",
    "@angular/cdk": "^17.3.5",
    "@angular/common": "^17.3.5",
    "@angular/compiler": "^17.3.5",
    "@angular/core": "^17.3.5",
    "@angular/forms": "^17.3.5",
    "@angular/material": "^17.3.5",
    "@angular/platform-browser": "^17.3.5",
    "@angular/platform-browser-dynamic": "^17.3.5",
    "@angular/router": "^17.3.5",
    "@myrmidon/auth-jwt-admin": "^4.0.0",
    "@myrmidon/auth-jwt-login": "^3.0.0",
    "@myrmidon/cadmus-refs-lookup": "^5.0.0",
    "@myrmidon/ng-mat-tools": "^3.0.0",
    "@myrmidon/ng-tools": "^3.0.0",
    "@myrmidon/paged-data-browsers": "^1.0.0",
    "@ngneat/effects-ng": "^3.1.3",
    "echarts": "^5.4.3",
    "gravatar": "^1.8.2",
    "ngx-echarts": "^17.1.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.5",
    "@angular/cli": "~17.3.5",
    "@angular/compiler-cli": "^17.3.5",
    "@angular/localize": "^17.3.5",
    "@types/jasmine": "~5.1.4",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "ngx-i18nsupport": "^0.17.1",
    "typescript": "~5.4.5"
  }
}
