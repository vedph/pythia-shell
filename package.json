{
  "name": "pythia-shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-cor": "ng build @myrmidon/pythia-core",
    "build-api": "ng build @myrmidon/pythia-api",
    "build-ui": "ng build @myrmidon/pythia-ui",
    "build-dcr": "ng build @myrmidon/pythia-document-reader",
    "build-doc": "ng build @myrmidon/pythia-document-list",
    "build-trm": "ng build @myrmidon/pythia-term-list",
    "build-sta": "ng build @myrmidon/pythia-stats",
    "build-qbl": "ng build @myrmidon/pythia-query-builder",
    "build-src": "ng build @myrmidon/pythia-search",
    "build-crp": "ng build @myrmidon/pythia-corpus-list",
    "build-lib": "npm run build-cor && npm run build-api && npm run build-ui && npm run build-dcr && npm run build-doc && npm run build-trm && npm run build-sta && npm run build-src && npm run build-crp && npm run build-qbl",
    "xi18n": "ng extract-i18n --output-path src/locale && xliffmerge --profile xliffmerge.json it-IT"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.2",
    "@angular/cdk": "^18.0.2",
    "@angular/common": "^18.0.2",
    "@angular/compiler": "^18.0.2",
    "@angular/core": "^18.0.2",
    "@angular/forms": "^18.0.2",
    "@angular/material": "^18.0.2",
    "@angular/platform-browser": "^18.0.2",
    "@angular/platform-browser-dynamic": "^18.0.2",
    "@angular/router": "^18.0.2",
    "@myrmidon/auth-jwt-admin": "^5.0.0",
    "@myrmidon/auth-jwt-login": "^5.0.0",
    "@myrmidon/cadmus-refs-lookup": "^5.1.1",
    "@myrmidon/ng-mat-tools": "^3.1.0",
    "@myrmidon/ng-tools": "^3.1.1",
    "@myrmidon/paged-data-browsers": "^1.1.0",
    "@ngneat/effects-ng": "^3.1.3",
    "echarts": "^5.5.0",
    "gravatar": "^1.8.2",
    "ngx-echarts": "^18.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.3",
    "@angular/cli": "~18.0.3",
    "@angular/compiler-cli": "^18.0.2",
    "@angular/localize": "^18.0.2",
    "@types/jasmine": "~5.1.4",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.0.0",
    "ngx-i18nsupport": "^0.17.1",
    "typescript": "~5.4.5"
  }
}
