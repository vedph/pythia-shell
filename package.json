{
  "name": "pythia-shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-cor": "ng build @myrmidon/pythia-core",
    "build-api": "ng build @myrmidon/pythia-api",
    "build-ui": "ng build @myrmidon/pythia-ui",
    "build-dcr": "ng build @myrmidon/pythia-document-reader",
    "build-doc": "ng build @myrmidon/pythia-document-list",
    "build-wrd": "ng build @myrmidon/pythia-word-index",
    "build-sta": "ng build @myrmidon/pythia-stats",
    "build-qbl": "ng build @myrmidon/pythia-query-builder",
    "build-src": "ng build @myrmidon/pythia-search",
    "build-crp": "ng build @myrmidon/pythia-corpus-list",
    "build-lib": "npm run build-cor && npm run build-api && npm run build-ui && npm run build-dcr && npm run build-doc && npm run build-wrd && npm run build-sta && npm run build-src && npm run build-crp && npm run build-qbl",
    "xi18n": "ng extract-i18n --output-path src/locale && xliffmerge --profile xliffmerge.json it"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.1",
    "@angular/cdk": "^19.0.1",
    "@angular/common": "^19.0.1",
    "@angular/compiler": "^19.0.1",
    "@angular/core": "^19.0.1",
    "@angular/forms": "^19.0.1",
    "@angular/material": "^19.0.1",
    "@angular/platform-browser": "^19.0.1",
    "@angular/platform-browser-dynamic": "^19.0.1",
    "@angular/router": "^19.0.1",
    "@myrmidon/auth-jwt-admin": "^6.0.0",
    "@myrmidon/auth-jwt-login": "^6.0.0",
    "@myrmidon/cadmus-refs-lookup": "^6.0.0",
    "@myrmidon/ng-mat-tools": "^4.0.0",
    "@myrmidon/ng-tools": "^4.0.0",
    "@myrmidon/paged-data-browsers": "^3.0.0",
    "echarts": "^5.4.3",
    "ngx-echarts": "^18.0.0",
    "rxjs": "~7.8.1",
    "ts-md5": "^1.3.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.2",
    "@angular/cli": "~19.0.2",
    "@angular/compiler-cli": "^19.0.1",
    "@angular/localize": "^19.0.1",
    "@types/jasmine": "~5.1.5",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.0.1",
    "ngx-i18nsupport": "^0.17.1",
    "typescript": "~5.5.2"
  }
}
