    "@angular/core": "^16.2.3",
    "@angular/forms": "^16.2.3",
    "@angular/material": "^16.2.2",
    "@angular/platform-browser": "^16.2.3",
    "@angular/platform-browser-dynamic": "^16.2.3",
    "@angular/router": "^16.2.3",
    "@myrmidon/auth-jwt-admin": "^2.0.0",
    "@myrmidon/auth-jwt-login": "^2.0.0",
    "@myrmidon/cadmus-refs-lookup": "^2.0.0",
    "@myrmidon/ng-mat-tools": "^2.0.0",
    "@myrmidon/ng-tools": "^2.0.0",
    "@ngneat/effects-ng": "^3.1.3",
    "@ngneat/elf": "^2.3.2",
    "@ngneat/elf-cli-ng": "^1.0.0",
    "@ngneat/elf-devtools": "^1.3.0",
    "@ngneat/elf-entities": "^4.4.4",
    "@ngneat/elf-pagination": "^1.1.0",
    "@ngneat/elf-requests": "^1.8.0",
    "echarts": "^5.4.2",
    "gravatar": "^1.8.2",
    "ngx-echarts": "^16.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.3",
    "@angular/localize": "^16.2.3",
    "@types/jasmine": "~4.3.4",
    "jasmine-core": "~5.0.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.1.0",
